{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "buildCommand": "bundle install && bundle exec jekyll build -d dist",
  "outputDirectory": "dist",
  "installCommand": "gem install bundler && bundle install"
}
